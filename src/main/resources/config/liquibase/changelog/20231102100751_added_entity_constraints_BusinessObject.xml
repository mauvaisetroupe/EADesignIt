        <addForeignKeyConstraint baseColumnNames="container_id"
                                 baseTableName="business_object"
                                 constraintName="fk_business_object__container_id"
                                 referencedColumnNames="id"
                                 referencedTableName="business_object"
                                 />
    </changeSet>
</databaseChangeLog>
